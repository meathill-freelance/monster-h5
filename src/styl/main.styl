@import "~animate.css/source/_base.css"
@import "~animate.css/source/fading_exits/fadeOutUp.css"

.sk-double-bounce.over
  .sk-child
    animation-iteration-count 1
    animation-name sk-doubleBounce-over

  .sk-double-bounce2
    animation-delay 0s
    animation-name sk-doubleBounce-over

@keyframes sk-doubleBounce-over
  0%, 100%
    -webkit-transform: scale(0);
    transform: scale(0);
    opacity 0

  50%
    -webkit-transform: scale(1);
    transform: scale(1);
