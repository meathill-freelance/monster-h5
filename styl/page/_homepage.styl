#homepage
  position relative

  .cover
    width 100%
    height 100%

    &.slideUp
      transition .5s transform
      transform translate3d(0,-100%,0)

  .wukong
    position absolute
    top 12.90%
    left 9%
    width 82%

    &.in
      transition 1s all

  .bufu
    position absolute
    top 32%
    left 30.8%
    width 38.4%

  .faxing
    position absolute
    top 60%
    left 21.6666667%
    width 56.666666%

  .bg
    position absolute
    top 0
    left 0
    width 100%
    z-index 0

  footer
    position absolute
    left 0
    right 0
    bottom 0
    padding 0 0 .8em
    font-size 1.5em

    i
      color white
      display block
      text-align center

  .nav
    width 100%
    height 100%
    background-repeat no-repeat
    background-size cover
    position absolute
    transition .5s opacity
    top 0
    overflow hidden

    &.backdrop
      opacity .6

    a
      position absolute

      img
        width 100%

    .about
      top 39px
      left 119px

    .chupin
      top 101px
      left 212px

    .intro
      top 161px
      left 120px

    .zhuchuang
      top 218px
